			"Common/WaveEngine.Physics/Projects/Windows/WaveEngine.Physics.csproj",
			"Extensions/WaveEngine.AI/Projects/Windows/WaveEngine.AI.csproj",
			"Extensions/WaveEngine.Analytics/Projects/Windows/WaveEngine.Analytics.csproj",
			"Extensions/WaveEngine.Cardboard/Projects/Windows/WaveEngine.Cardboard.csproj",
			"Extensions/WaveEngine.Dolby/Projects/Windows/WaveEngine.Dolby/WaveEngine.Dolby.csproj",
			"Extensions/WaveEngine.Hololens/Projects/WaveEngine.Hololens.csproj",
