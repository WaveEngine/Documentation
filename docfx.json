			"Engine/Extensions/WaveEngine.Spine/Projects/Windows/WaveEngine.Spine.csproj",
			"Extensions/WaveEngine.TiledMap/Projects/Windows/WaveEngine.TiledMap.csproj",
			"Extensions/WaveEngine.Vuforia/Projects/Windows/WaveEngine.Vuforia.csproj"					
          ],
		  "src":"../"
         ,
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "recipes/**.md",
          "recipes/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "recipes/**/images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md",
		  "api/**"
		  ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../DocOnline",    
	"globalMetadata":{
		"_appTitle": "Wave Engine API Doc",
		"_enableSearch": "true",
		"_gitContribute": {
			"repo":"https://github.com/juanlao/Documentation.git",
			"branch": "develop"
		}
      },
    "fileMetadataFiles": [],
    "template": [
      "WaveTemplate"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false
  }
}