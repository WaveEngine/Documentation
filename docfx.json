			"Extensions/WaveEngine.Networking/Projects/Windows/WaveEngine.Networking.csproj",
			"Extensions/WaveEngine.NoesisGUI/Projects/Windows/WaveEngine.NoesisGUI.csproj",
			"Extensions/WaveEngine.OculusRift/WaveEngine.OculusRift.csproj",
			"Extensions/WaveEngine.OpenVR/Project/WaveEngine.OpenVR.csproj",
